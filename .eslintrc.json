{
	"env": {
		"node": true,
		"es6": true
	},
	"parserOptions": { "ecmaVersion": 2017 },
	"extends": "eslint:recommended",
	"rules": {
		"no-console": "off",
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"semi": [
			"error",
			"always"
		],
		"prefer-const": "error",
		"prefer-destructuring": "error",
		"no-var": "error",
		"strict": "error",
		"eol-last": "error",
		"max-len": "error",
		"no-unused-expressions": "error"
	}
}